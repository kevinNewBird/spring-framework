<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="replacer" class="com.mashibing.analysis.instanceCreation.methodOverride.replace.MethodBeanReplace"/>
	<bean name="testBean" class="com.mashibing.analysis.instanceCreation.methodOverride.replace.MethodBean">
		<replaced-method name="test" replacer="replacer"/>
	</bean>

</beans>